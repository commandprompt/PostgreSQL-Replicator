"    | VALID UNTIL 'timestamp' \n"
"    | IN ROLE nombre_de_rol [, ...]\n"
"    | IN GROUP nombre_de_rol [, ...]\n"
"    | ROLE nombre_de_rol [, ...]\n"
"    | ADMIN nombre_de_rol [, ...]\n"
"    | USER nombre_de_rol [, ...]\n"
"    | SYSID uid"

