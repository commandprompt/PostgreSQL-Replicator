"    [ (nombre_columna [, ...] ) ]\n"
"    [ WITH (parámetro_de_almacenamiento [= valor] [, ... ]) | WITH OIDS | "
"WITHOUT OIDS ]\n"
"    [ ON COMMIT { PRESERVE ROWS | DELETE ROWS | DROP } ]\n"
"    [ TABLESPACE tablespace ]\n"
