#: sql_help.h:30
msgid ""
"ALTER AGGREGATE name ( type [ , ... ] ) RENAME TO new_name\n"
"ALTER AGGREGATE name ( type [ , ... ] ) OWNER TO new_owner\n"
"ALTER AGGREGATE name ( type [ , ... ] ) SET SCHEMA new_schema"
